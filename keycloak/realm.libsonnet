{
  apiVersion: 'legacy.k8s.keycloak.org/v1alpha1',
  kind: 'KeycloakRealm',
  metadata: {
    name: 'example-keycloakrealm',
    labels: {
      app: 'external-sso',
    },
  },
  spec: {
    instanceSelector: {
      matchLabels: {
        app: 'external-sso',
      },
    },
    realm: {
      id: 'basic',
      realm: 'basic',
      enabled: true,
      displayName: 'Basic Realm',
      adminEventsEnabled: true,
      eventsEnabled: true,
      eventsListeners: [
        'jboss-logging',
      ],
      enabledEventTypes: [
        'SEND_RESET_PASSWORD',
        'UPDATE_CONSENT_ERROR',
        'GRANT_CONSENT',
        'REMOVE_TOTP',
        'REVOKE_GRANT',
